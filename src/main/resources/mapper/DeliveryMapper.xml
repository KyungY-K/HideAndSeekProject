<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="DeliveryDAO">
  
  	<select id="deli_cont" parameterType="String" resultType="DeliveryDTO">
 	 	select * from delivery where user_id = #{userId}
 	 </select>
 	 
 	 <select id="delivery" parameterType="int" resultType="DeliveryDTO">
 	 	select * from delivery where deli_no = #{deli_no}
 	 </select>
  
  
  
  
  
  </mapper>